{

    "src": [
      "../iPhoto-src/"
    ],

    "dest": {
        "overwrite": false,

        "error": "../iPhoto-dest/error.json",

        "photo": "../iPhoto-dest/photo/{make}/{model}/{year}/{year}-{month}-{day}/{name}",

        ".jpg": "../iPhoto-dest/jpg/{date}/{name}",
        ".png": "../iPhoto-dest/png/{date}/{name}",
        ".mov": "../iPhoto-dest/mov/{date}/{name}",
        ".mp4": "../iPhoto-dest/mp4/{date}/{name}",

        ".*": "../iPhoto-dest/other/{date}/{name}"
        
    },

    "excludes": [
        ".ini"
    ],

    "exif": {
        "exts": [".JPG"]
    }

}
